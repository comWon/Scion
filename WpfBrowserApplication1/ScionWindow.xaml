<Window x:Class="Scion.Wpf.ScionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Scion.Wpf"
        mc:Ignorable="d"
        Title="ScionWindow" Height="327.907" Width="426.453">
    <Grid Margin="0,0,-8,-31">
        <GridSplitter x:Name="gridSplitter" HorizontalAlignment="Left" Height="325" Margin="95,-28,0,0" VerticalAlignment="Top" Width="0"/>
        <Frame x:Name="masterFrame" Content="Frame" HorizontalAlignment="Left" Height="300" Margin="95,0,0,0" VerticalAlignment="Top" Width="300" Navigated="frame_Navigated" Template="{DynamicResource FrameControlTemplate1}" Source="/ScionInit;component/ScionLoader.xaml"/>
        <Button x:Name="MonsterButton" Content="Monsters" HorizontalAlignment="Left" Margin="0,17,0,0" VerticalAlignment="Top" Width="75" Click="MonsterButton_Click"/>
        <Button x:Name="ScionButton" Content="Scions" HorizontalAlignment="Left" Margin="0,42,0,0" VerticalAlignment="Top" Width="75" Click="ScionButton_Click"/>
        <Button x:Name="Game" Content="Run Combat" HorizontalAlignment="Left" Margin="0,67,0,0" VerticalAlignment="Top" Width="75" Click="Game_Click"/>

    </Grid>
</Window>
